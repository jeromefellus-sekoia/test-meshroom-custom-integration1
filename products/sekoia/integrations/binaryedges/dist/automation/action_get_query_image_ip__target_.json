                "timezone": {
                  "type": "string"
                },
                "latitude": {
                  "type": "number"
                },
                "longitude": {
                  "type": "number"
                },
                "location": {
                  "type": "array",
                  "items": {
                    "type": "number"
                  }
                },
                "country_name": {
                  "type": "string"
                },
                "city_name": {
                  "type": "string"
                }
              }
