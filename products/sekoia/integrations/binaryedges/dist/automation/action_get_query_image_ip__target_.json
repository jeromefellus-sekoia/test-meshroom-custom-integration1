    "title": "List screenshots",
    "properties": {
      "target": {
        "in": "path",
        "type": "string"
      },
      "page": {
        "in": "query",
        "type": "integer"
      }
    },
    "required": [
      "target"
    ]
  },
  "results": {
    "type": "object",
    "properties": {
      "pagesize": {
        "type": "integer"
      },
      "query": {
        "type": "string"
      },
      "total": {
        "type": "integer"
      },
      "page": {
        "type": "integer"
      },
      "events": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "thumb": {
              "type": "string"
            },
            "tags": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "ts": {
              "type": "integer"
            },
            "port": {
              "type": "integer"
            },
            "url": {
              "type": "string"
            },
            "height": {
              "type": "integer"
            },
            "geoip": {
              "type": "object",
