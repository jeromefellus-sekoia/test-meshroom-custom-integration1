{
  "uuid": "1ba3002f-d294-4415-8840-ac2de7f35519",
  "name": "Get History",
  "docker_parameters": "get-query/ip/historical/{target}",
  "description": "Details about an Host, with data up to 6 months.\n\n List of events for the specified host, with events for each time that:\n - A port was detected open\n - A service was found running\n - Other modules were successfully executed",
  "arguments": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Get History",
    "properties": {
      "target": {
        "in": "path",
        "type": "string"
      }
    },
    "required": [
      "target"
    ]
  },
  "results": {
    "type": "object",
    "properties": {
      "total": {
        "type": "integer"
      },
      "query": {
        "type": "string"
      },
      "events": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "target": {
              "type": "object",
              "properties": {
                "ip": {
                  "type": "string"
                },
                "protocol": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                }
              }
            },
            "origin": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                },
                "ts": {
                  "type": "integer"
                }
              }
            },
            "result": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "state": {
                      "type": "object",
                      "properties": {
                        "state": {
                          "type": "string"
                        }
                      }
                    },
                    "service": {
                      "type": "object",
                      "properties": {
                        "product": {
                          "type": "string"
                        },
                        "banner": {
                          "type": "string"
                        },
                        "name": {
                          "type": "string"
                        },
                        "method": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}