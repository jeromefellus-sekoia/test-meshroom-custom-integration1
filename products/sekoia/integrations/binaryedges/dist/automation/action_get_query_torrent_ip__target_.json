{
  "uuid": "50be5505-aea4-443a-849b-de3bde010d4a",
  "name": "Get torrent activity",
  "docker_parameters": "get-query/torrent/ip/{target}",
  "description": "Details about torrents transferred by an Host. List of recent torrent events for the specified host, including details of the peer and torrent. See [Torrent Data](https://docs.binaryedge.io/torrent/) for more details.",
  "arguments": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Get torrent activity",
    "properties": {
      "target": {
        "in": "path",
        "type": "string"
      }
    },
    "required": [
      "target"
    ]
  },
  "results": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string"
      },
      "total": {
        "type": "integer"
      },
      "events": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "origin": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                },
                "module": {
                  "type": "string"
                },
                "ts": {
                  "type": "integer"
                }
              }
            },
            "peer": {
              "type": "object",
              "properties": {
                "ip": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                }
              }
            },
            "torrent": {
              "type": "object",
              "properties": {
                "infohash": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "source": {
                  "type": "string"
                },
                "category": {
                  "type": "string"
                },
                "subcategory": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}