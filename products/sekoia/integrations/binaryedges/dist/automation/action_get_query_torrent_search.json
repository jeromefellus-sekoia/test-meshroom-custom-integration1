{
  "uuid": "c9d5879f-15ed-4249-8a5d-fff108238dc6",
  "name": "Search torrent activities",
  "docker_parameters": "get-query/torrent/search",
  "description": "Events based on a Query. List of recent events for the given query, including details of the peer and torrent. Can be used with specific parameters and/or full-text search.",
  "arguments": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Search torrent activities",
    "properties": {
      "query": {
        "in": "query",
        "type": "string"
      },
      "page": {
        "in": "query",
        "type": "integer"
      }
    },
    "required": [
      "query"
    ]
  },
  "results": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string"
      },
      "page": {
        "type": "integer"
      },
      "pagesize": {
        "type": "integer"
      },
      "total": {
        "type": "integer"
      },
      "events": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "origin": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                },
                "module": {
                  "type": "string"
                },
                "ts": {
                  "type": "integer"
                }
              }
            },
            "node": {
              "type": "object",
              "properties": {
                "ip": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                }
              }
            },
            "peer": {
              "type": "object",
              "properties": {
                "ip": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                }
              }
            },
            "torrent": {
              "type": "object",
              "properties": {
                "infohash": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "source": {
                  "type": "string"
                },
                "category": {
                  "type": "string"
                },
                "subcategory": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}