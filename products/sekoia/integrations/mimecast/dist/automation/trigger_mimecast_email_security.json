{
  "description": "Mimecast - Email Security",
  "docker_parameters": "mimecast_email_security",
  "name": "[BETA] Fetch new email events from Mimecast",
  "arguments": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "frequency": {
        "type": "integer",
        "description": "Batch frequency in seconds",
        "default": 60
      },
      "chunk_size": {
        "type": "integer",
        "description": "The size of chunks for the batch processing",
        "default": 100,
        "minimum": 1,
        "maximum": 100
      },
      "intake_server": {
        "description": "Server of the intake server (e.g. 'https://intake.sekoia.io')",
        "default": "https://intake.sekoia.io",
        "type": "string"
      },
      "intake_key": {
        "description": "Intake key to use when sending events",
        "type": "string"
      }
    },
    "required": [
      "intake_key"
    ],
    "secrets": [
      "token"
    ],
    "title": "The configuration to fetch new events",
    "type": "object"
  },
  "results": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {},
    "title": "The events"
  },
