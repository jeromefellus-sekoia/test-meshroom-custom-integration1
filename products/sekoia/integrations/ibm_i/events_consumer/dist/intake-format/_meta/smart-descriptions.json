[
  {
    "value": "{event.action} from process {process.name} with pid {process.pid}",
    "conditions": [
      {
        "field": "event.action"
      },
      {
        "field": "process.name"
      },
      {
        "field": "process.pid"
      }
    ]
  },
  {
    "value": "{ibm_i.pgmName} responded with {event.reason}",
    "conditions": [
      {
        "field": "ibm_i.pgmName"
      },
      {
        "field": "event.reason"
      }
    ]
  },
  {
    "value": "{event.reason} by user {user.name}",
    "conditions": [
      {
        "field": "user.name"
      },
      {
        "field": "event.reason"
      }
    ]
  }
]
