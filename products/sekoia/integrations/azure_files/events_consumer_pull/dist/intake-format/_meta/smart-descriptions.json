      },
      {
        "field": "url.path"
      }
    ]
  },
  {
    "value": "{source.address} read the file {url.path}",
    "conditions": [
      {
        "field": "event.dataset",
        "value": "StorageRead"
      },
      {
        "field": "source.address"
      },
      {
        "field": "url.path"
      }
    ]
  },
  {
    "value": "New {azure.files.status} event on uri {url.original}",
